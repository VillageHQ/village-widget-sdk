  "include": [
    "./config/"
  ],
  "compilerOptions": {    
    "allowJs": true,
    "checkJs": true,
    "target": "es2016", 
    "module": "commonjs", 
    "esModuleInterop": true,      
    "forceConsistentCasingInFileNames": true, 
    "strict": true,                     
    "skipLibCheck": true 
  }
}
